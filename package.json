{
  "name": "yajsapi",
  "version": "0.7.0",
  "description": "NodeJS API for Next Golem",
  "repository": "https://github.com/golemfactory/yajsapi",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:browser": "tsc && rollup --config rollup.config.js",
    "clean": "rimraf node_modules/ dist/ || echo 'Project is already clean'",
    "docs": "typedoc yajsapi/ --plugin none",
    "docs:md": "typedoc yajsapi/ --plugin typedoc-plugin-markdown",
    "docs:clean": "rimraf docs/ && run-s handbook:clean",
    "test": "ts-mocha -p tsconfig.json 'tests/+(unit|integration)/**.+(spec|test).ts'",
    "test:unit": "ts-mocha -p tsconfig.json tests/unit/**.test.ts",
    "test:integration": "ts-mocha -p tsconfig.json --file tests/integration/setup.ts tests/integration/**.spec.ts",
    "test:cypress": "cypress run",
    "cov": "nyc --cache false --exclude tests ts-mocha -p tsconfig.json --file tests/integration/setup.ts 'tests/+(unit|integration)/**.+(spec|test).ts'",
    "cov:integration": "nyc --cache false --exclude tests ts-mocha -p tsconfig.json --file tests/integration/setup.ts tests/integration/**.spec.ts",
    "cov:unit:html": "nyc --reporter=html --cache false ts-mocha -p tsconfig.json --file tests/integration/setup.ts tests/unit/**.test.ts",
    "cov:integration:html": "nyc --reporter=html --cache false ts-mocha -p tsconfig.json --file tests/integration/setup.ts tests/integration/**.spec.ts",
    "cov:all:html": "nyc --reporter=html --cache false ts-mocha -p tsconfig.json --file tests/integration/setup.ts 'tests/+(unit|integration)/**.+(spec|test).ts'",
    "cov:lcov": "nyc  --reporter=lcov --cache false ts-mocha -p tsconfig.json --file tests/integration/setup.ts tests/unit/**.test.ts",
    "lint": "eslint .",
    "format": "prettier yajsapi/",
    "handbook:clean": "rimraf handbook/ handbook_gen/.env",
    "handbook:py": "python3 -m venv handbook_gen/.env && source handbook_gen/.env/bin/activate",
    "handbook:poetry": "pip install -U pip && pip install poetry && cd handbook_gen && poetry install",
    "handbook:summary": "python handbook_gen/handbook_gen.py --overwrite",
    "handbook": "run-s docs:md && run-s handbook:py && run-s handbook:poetry && run-s handbook:summary"
  },
  "files": [
    "dist"
  ],
  "author": "Muhammed Tanrikulu <muhammed@golem.network>",
  "license": "LGPL-3.0",
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "axios": "^1.1.3",
    "eventsource": "^2.0.2",
    "ip-num": "^1.4.1",
    "winston": "^3.8.2",
    "ya-ts-client": "^0.5.3"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@rauschma/stringio": "^1.4.0",
    "@rollup/plugin-alias": "^4.0.2",
    "@rollup/plugin-commonjs": "^23.0.2",
    "@rollup/plugin-inject": "^5.0.2",
    "@rollup/plugin-json": "^5.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@types/eccrypto": "^1.1.3",
    "@types/mocha": "^10.0.0",
    "@types/node": "18",
    "@types/utf8": "^3.0.1",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "buffer": "^6.0.3",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "chai-uuid": "^1.0.6",
    "cypress": "11.0.1",
    "eslint": "~8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "mocha": "^10.1.0",
    "node-stdlib-browser": "^1.2.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "open-cli": "^7.1.0",
    "prettier": "^2.7.1",
    "process": "^0.11.10",
    "rewiremock": "^3.14.4",
    "rollup": "^3.3.0",
    "rollup-plugin-hypothetical": "^2.1.1",
    "rollup-plugin-ignore": "^1.0.10",
    "rollup-plugin-ignore-import": "^1.3.2",
    "rollup-plugin-uglify": "^6.0.4",
    "rollup-plugin-visualizer": "^5.8.3",
    "ts-loader": "^9.4.1",
    "ts-mocha": "^10.0.0",
    "tsconfig-paths": "^4.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.23.21",
    "typedoc-plugin-markdown": "^3.13.6",
    "typescript": "^4.8.4",
    "why-is-node-running": "^2.2.2"
  }
}
